  "scripts": {
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"api,web\" \"nx serve api-core\" \"nx serve web-shell\"",
    "dev:api": "nx serve api-core",
    "dev:web": "nx serve web-shell",
    "build": "npm run generate:context && nx build api-core && nx build web-shell",
    "build:api": "nx build api-core",
    "build:web": "nx build web-shell",
    "test": "nx test api-core && nx test contracts",
    "test:api": "nx test api-core",
    "test:contracts": "nx test contracts",
    "lint": "nx lint api-core && nx lint web-shell && nx lint contracts",
    "typecheck": "nx typecheck api-core && nx typecheck contracts",
    "generate:context": "node scripts/generate-project-context.js"
  },
